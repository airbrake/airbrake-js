{
    "compilerOptions": {
        "lib": ["ES2015", "DOM"],
        "target": "ES6",
        "moduleResolution": "node",
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "skipLibCheck": true
    }
}
